{
  "name": "feud-game",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev-next": "next dev",
    "dev-socket": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "eslint",
    "import-questions": "node scripts/import-questions.js",
    "import-sample": "node scripts/import-questions.js scripts/sample-questions.json",
    "import-expanded": "node scripts/import-questions.js scripts/expanded-questions.json",
    "create-teams": "node scripts/create-sample-teams.js",
    "setup-game": "npm run create-teams && npm run import-sample",
    "setup-atlas": "node scripts/setup-atlas-db.js",
    "test-scoring": "node -e \"const { scoringEngine } = require('./lib/scoring.js'); console.log('Scoring system loaded successfully:', scoringEngine.getGameState());\"",
    "test-api": "node test/api-test.js",
    "test-curl": "bash test/test-with-curl.sh",
    "test-db": "node test-db.js"
  },
  "dependencies": {
    "next": "16.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "mongodb": "^6.3.0",
    "mongoose": "^8.1.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
